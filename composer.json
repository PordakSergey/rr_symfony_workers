  "license": "MIT",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "yalop",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "symfony/config": "^7.0 || ^8.0",
    "symfony/dependency-injection": "^7.0 || ^8.0",
    "symfony/yaml": "^7.0 || ^8.0",
    "symfony/http-client": "^7.0 || ^8.0",
    "symfony/framework-bundle": "^7.0 || ^8.0",
    "symfony/runtime": "^7.0 || ^8.0",
    "symfony/messenger": "^7.0 || ^8.0",
    "symfony/serializer-pack": "^1.3",
    "symfony/var-dumper": "^7.0 || ^8.0",
    "symfony/var-exporter": "^7.0 || ^8.0",
    "symfony/filesystem": "7.*",
    "spiral/roadrunner": "^2025.1.0",
    "spiral/roadrunner-worker": "^3.0.0",
    "spiral/goridge": "^4.0",
    "spiral/roadrunner-http": "^3.6",
    "spiral/roadrunner-grpc": "^3.5.2",
    "spiral/roadrunner-kv": "^4.0",
    "spiral/roadrunner-jobs": "^4.7",
    "nyholm/psr7": "^1.8",
    "doctrine/doctrine-bundle": "^3.0",
    "doctrine/orm": "^3.0",
    "temporal/sdk": "2.16.0"
